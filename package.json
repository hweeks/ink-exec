{
  "name": "ink-exec",
  "version": "1.0.0",
  "description": "an exec wrapper for ink uis",
  "main": "dist/index.js",
  "repository": "git@github.com:hweeks/ink-exec.git",
  "author": "hammy-late-nite <git@hweeks.com>",
  "license": "WTFPL",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "example": "yarn bb example/",
    "lint": "eslint ./ --ext .js,.tsx,.ts",
    "format:write": "prettier --write ./src ./__tests__",
    "format:check": "prettier --check ./src ./__tests__",
    "test": "jest",
    "test:debug": "node debug.js",
    "test:debug:nix": "node --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache --watchAll",
    "test:debug:win": "node --inspect-brk ./node_modules/jest-cli/bin/jest.js --runInBand --no-cache --watchAll",
    "dev": "nodemon -e ts,tsx --exec yarn bb",
    "dev:brk": "nodemon -e ts,tsx --exec yarn bbb",
    "bbb": "babel-node --inspect-brk --extensions \".js,.ts,.tsx\"",
    "bb": "babel-node --inspect --extensions \".js,.ts,.tsx\"",
    "cli": "babel-node --extensions \".js,.ts,.tsx\" ./src/index.tsx",
    "prepublishOnly": "tsc",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/node": "7.13.13",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.8",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.22",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "commitizen": "4.2.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.24.0",
    "husky": "6.0.0",
    "ink-testing-library": "^2.1.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "semantic-release": "17.2.4",
    "typescript": "4.2.4",
    "vorta": "1.1.0"
  },
  "dependencies": {
    "ink": "3.0.8",
    "react": "17.0.2",
    "strip-ansi": "7.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
